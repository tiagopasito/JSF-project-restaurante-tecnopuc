package br.com.dbserver_restaurante.modelo;

public class Restaurante {

	private Long id = 0l;
	private String nome;

	
	public Restaurante(String nome, long id) {
		this.nome = nome;
		this.id = id;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;	
	}	
	
	public String getNome() {
		return nome;
	}
}
